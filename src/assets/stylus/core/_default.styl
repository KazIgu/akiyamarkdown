normalize-css()

*
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)
  -webkit-touch-callout none

html
  font-size 62.5%

body
  font-size 1.4em
  letter-spacing 0.05em
  line-height 1.7

h1
  margin 0

p
  margin 0
  padding 0

ul
  list-style none
  padding 0
  margin 0

input, textarea
  outline none


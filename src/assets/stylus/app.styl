@import './core/_import'
@import './block/*'

html, body
  overflow hidden

#editor
  width 100%
  height 100vh
  font 14px/normal 'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', 'source-code-pro', monospace

.ace-monokai
  .ace_title
    color #70aacc

  .ace_subtitle
    color #fd9a41

  .ace_smalltitle
    color #b6d971

  .ace_indent
    color #aaaaaa
